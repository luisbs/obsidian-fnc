{
    "license": "MIT",
    "version": "1.4.6",
    "name": "@luis.bs/obsidian-fnc",
    "description": "Common obsidian utility functions",
    "keywords": [
        "obsidian",
        "utility"
    ],
    "author": {
        "name": "Luis Brayan",
        "url": "https://github.com/luisbs"
    },
    "repository": {
        "url": "https://github.com/luisbs/obsidian-fnc"
    },
    "publishConfig": {
        "provenance": true,
        "access": "public"
    },
    "files": [
        "lib/**/*"
    ],
    "type": "module",
    "exports": {
        ".": {
            "types": "./lib/index.d.ts",
            "import": "./lib/index.js"
        },
        "./lib/*": {
            "types": "./lib/*.d.ts",
            "import": "./lib/*.js"
        }
    },
    "scripts": {
        "prepare": "simple-git-hooks",
        "lint:eslint": "eslint",
        "lint:prettier": "prettier --check src",
        "lint": "pnpm lint:eslint && pnpm lint:prettier",
        "test": "vitest",
        "watch": "tsc --watch",
        "build": "tsc",
        "ci:publish": "pnpm build && clean-pkg-json && changeset publish",
        "release": "changeset"
    },
    "dependencies": {
        "@floating-ui/dom": "^1.6.13"
    },
    "devDependencies": {
        "@changesets/cli": "^2.28.1",
        "@commitlint/cli": "^19.8.0",
        "@commitlint/config-conventional": "^19.8.0",
        "@eslint/js": "^9.23.0",
        "@types/node": "^22.13.14",
        "clean-pkg-json": "^1.2.1",
        "esbuild": "^0.25.2",
        "eslint": "^9.23.0",
        "eslint-config-prettier": "^10.1.1",
        "globals": "^16.0.0",
        "lint-staged": "^15.5.0",
        "obsidian": "^1.8.7",
        "prettier": "^3.5.3",
        "simple-git-hooks": "^2.12.1",
        "tslib": "^2.8.1",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.28.0",
        "vitest": "^3.0.9"
    },
    "simple-git-hooks": {
        "pre-commit": "npx lint-staged",
        "commit-msg": "npx commitlint -e -V"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "lint-staged": {
        "src/**/*.ts": [
            "eslint",
            "prettier"
        ]
    }
}
